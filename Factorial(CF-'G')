#include<iostream>
#include<vector>
using namespace std;

long long fact1(int x)
{
    if(x==1 || x==0)
    {
        return 1;
    }
    else
        return x*fact1(x-1);
}

int main()
{
    int n;
    cin>> n;

    vector<int> A(n);
    for(int i=0;i<n;i++)
    {
        cin>> A[i];

    }


    for(int i=0;i<n;i++)
    {
        long long result = fact1(A[i]);
        cout<<result<<endl;
    }


}
