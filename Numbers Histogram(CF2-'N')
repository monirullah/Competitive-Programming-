#include<iostream>
#include<vector>
using namespace std;

int main()
{
    char s;
    cin>>s;
    int n,m;
    cin>>n;
    vector<long long> A(n);
    for(int i=0;i<n;i++)
    {
        cin>> A[i];
    }
    for(int i=0;i<n;i++)
    {
        m=A[i];
          for(int j=1;j<=m;j++)
            {
                cout<< s;
            }
            cout<< endl;

    }

}
