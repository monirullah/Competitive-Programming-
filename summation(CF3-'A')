#include<iostream>
#include<vector>
#include<cmath>
using namespace std;

int main()
{
    int n;
    cin>> n;
    vector<long long> A(n);

    long long sum=0;
    for(int i=0;i<n;i++)
    {
        cin>> A[i];
    }

    for(int i=0;i<n;i++)
    {
        sum = sum + A[i];
    }

    cout<< abs(sum) << endl;;


}
